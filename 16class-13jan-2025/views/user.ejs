<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('head') %>
</head>
<body>
<header>
    <%- include('header') %>
</header>
    
<main>
    
    <table class="table">
      <thead>

        <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">age</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(user) { %>

      
        <tr>
    
                    <td> <%= user.id  %></td>
                    <td>  <%= user.name %></td>
                    <td>  <%= user.age %></td>
    
    
                </tr>
            <% }); %>

          </tbody>
    </table>
</main>
<footer>
    <%- include('footer');%>
    
</footer>
</body>
</html>